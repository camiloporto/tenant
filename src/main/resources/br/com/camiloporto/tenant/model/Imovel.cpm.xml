<?xml version="1.0"?>
<!DOCTYPE compass-core-mapping PUBLIC
    "-//Compass/Compass Core Mapping DTD 2.2//EN"
    "http://www.compass-project.org/dtd/compass-core-mapping-2.2.dtd">

<compass-core-mapping package="br.com.camiloporto.tenant.model">

  <class name="Imovel" alias="Imovel">

    <id name="tipo" >
    	<meta-data>id</meta-data>
    </id>

    <property name="rua">
      <meta-data>rua</meta-data>
    </property>
    
    <property name="estado">
      <meta-data>estado</meta-data>
    </property>
    
    <property name="cidade">
      <meta-data>cidade</meta-data>
    </property>
    
    <property name="bairro">
      <meta-data>bairro</meta-data>
    </property>
    
    <property name="complemento">
      <meta-data>complemento</meta-data>
    </property>

  </class>
</compass-core-mapping>